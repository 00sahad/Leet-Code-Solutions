// Check If N and Its Double Exist


/**
 * @param {number[]} arr
 * @return {boolean}
 */
var checkIfExist = function (arr) {
    for (var i = 0; i < arr.length; i++) {
        if (arr[i] !== 0 && arr.includes(arr[i] * 2)) return true;
        else if(arr[i]==0 && arr.lastIndexOf(arr[i])!== i) return true
    }
    return false
};